@article{helberExpressionFluorescenceMarkers2008,
  title = {Expression of the Fluorescence Markers {{DsRed}} and {{GFP}} Fused to a Nuclear Localization Signal in the Arbuscular Mycorrhizal Fungus {{Glomus}} Intraradices},
  author = {Helber, Nicole and Requena, Natalia},
  year = {2008},
