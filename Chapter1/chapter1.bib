@article{rouphaelArbuscularMycorrhizalFungi2015,
  title = {Arbuscular Mycorrhizal Fungi Act as Biostimulants in Horticultural Crops},
  author = {Rouphael, Youssef and Franken, Philipp and Schneider, Carolin and Schwarz, Dietmar and Giovannetti, Manuela and Agnolucci, Monica and Pascale, Stefania De and Bonini, Paolo and Colla, Giuseppe},
  year = {2015},
  month = nov,
  journal = {Scientia Horticulturae},
  series = {Biostimulants in {{Horticulture}}},
  volume = {196},
  pages = {91--108},
  issn = {0304-4238},
  doi = {10.1016/j.scienta.2015.09.002},
  abstract = {In the coming years, more sustainable horticultural practices should be developed to guarantee greater yield and yield stability, in order to meet the increasing food global demand. An environmentally-friendly way to achieve the former objectives is represented by the biostimulant functions displayed by arbuscular mycorrhizal fungi (AMF). AMF support plant nutrition by absorbing and translocating mineral nutrients beyond the depletion zones of plant rhizosphere (biofertilisers) and induce changes in secondary metabolism leading to improved nutraceutical compounds. In addition, AMF interfere with the phytohormone balance of host plants, thereby influencing plant development (bioregulators) and inducing tolerance to soil and environmental stresses (bioprotector). Maximum benefits from AMF activity will be achieved by adopting beneficial farming practices (e.g. reduction of chemical fertilisers and biocides), by inoculating efficient AMF strains and also by the appropriate selection of plant host/fungus combinations. This review gives an up to date overview of the recent advances in the production of quality AMF inocula and in the biostimulant properties of AMF on plant health, nutrition and quality of horticultural crops (fruit trees, vegetables, flower crops and ornamentals). The agronomical, physiological and biochemical processes conferring tolerance to drought, salinity, nutrient deficiency, heavy metal contaminations and adverse soil pH in mycorrhizal plants are encompassed. In addition, the influence of bacterial interactions and farm management on AMF is discussed. Finally, the review identifies several future research areas relevant to AMF to exploit and improve the biostimulant effects of AMF in horticulture.},
  langid = {english},
  keywords = {Abiotic stresses,Biofertilisers,Horticulture,Phytochemical compounds,spp.,Sustainability},
  file = {/home/shawna/Dropbox/zotero_library/rouphael_et_al_2015_arbuscular_mycorrhizal_fungi_act_as_biostimulants_in_horticultural_crops.pdf}
}

@article{roweCoercionEvolutionPlant2018,
  title = {Coercion in the {{Evolution}} of {{Plant}}\textendash{{Microbe Communication}}: {{A Perspective}}},
  shorttitle = {Coercion in the {{Evolution}} of {{Plant}}\textendash{{Microbe Communication}}},
  author = {Rowe, S. L. and Norman, J. S. and Friesen, M. L.},
  year = {2018},
  month = aug,
  journal = {Molecular Plant-Microbe Interactions\textregistered},
  volume = {31},
  number = {8},
  pages = {789--794},
  publisher = {{Scientific Societies}},
  issn = {0894-0282},
  doi = {10.1094/MPMI-11-17-0276-CR},
  abstract = {Plants and microbes are dependent on chemical signals as a means of interkingdom communication. There are two predicted paths for the evolution of these signals. Ritualization is the oft-assumed pathway for the evolution of plant\textendash microbe communication systems. In this process, chemical signals, which benefit both receiver and sender, evolve from chemical cues, which benefit only the receiver. However, plant\textendash microbe signaling may evolve from coercive interactions as well, a process known as sensory manipulation. Here, we aim to highlight the prevalence of coercive interactions and discuss sensory manipulation in the context of plant\textendash microbe interactions. We present two examples of stabilized coercion: microbial coercion of plants via the release of phytohormones and plant coercion of microbes via manipulation of quorum-sensing compounds. Furthermore, we provide an evolutionary framework for the emergence of signaling from coercive plant\textendash microbe interactions through the process of sensory manipulation. We hope that researchers will recognize the relevance of coercive interactions in plant\textendash microbe systems and consider sensory manipulation as a plausible evolutionary trajectory for the emergence of plant\textendash microbe signaling.},
  copyright = {Creative Commons Attribution-NonCommercial-ShareAlike 4.0 International License (CC-BY-NC-SA)},
  file = {/home/shawna/Dropbox/zotero_library/rowe_et_al_2018_coercion_in_the_evolution_of_plant–microbe_communication_-_a_perspective.pdf}
}

@article{ruaHomefieldAdvantageEvidence2016,
  title = {Home-Field Advantage? Evidence of Local Adaptation among Plants, Soil, and Arbuscular Mycorrhizal Fungi through Meta-Analysis},
  shorttitle = {Home-Field Advantage?},
  author = {R{\'u}a, Megan A. and Antoninka, Anita and Antunes, Pedro M. and Chaudhary, V. Bala and Gehring, Catherine and Lamit, Louis J. and Piculell, Bridget J. and Bever, James D. and Zabinski, Cathy and Meadow, James F. and Lajeunesse, Marc J. and Milligan, Brook G. and Karst, Justine and Hoeksema, Jason D.},
