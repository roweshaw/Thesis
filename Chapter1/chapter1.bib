@article{giovannettiPatternsBelowgroundPlant2004,
  title = {Patterns of Below-Ground Plant Interconnections Established by Means of Arbuscular Mycorrhizal Networks},
  author = {Giovannetti, Manuela and Sbrana, Cristiana and Avio, Luciano and Strani, Patrizia},
  year = {2004},
  journal = {New Phytologist},
  volume = {164},
  number = {1},
