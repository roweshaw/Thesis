@article{wipfTradingArbuscularMycorrhiza2019,
  title = {Trading on the Arbuscular Mycorrhiza Market: From Arbuscules to Common Mycorrhizal Networks},
  shorttitle = {Trading on the Arbuscular Mycorrhiza Market},
  author = {Wipf, Daniel and Krajinski, Franziska and {van Tuinen}, Diederik and Recorbet, Ghislaine and Courty, Pierre-Emmanuel},
