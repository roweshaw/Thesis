@article{quan2008,
  title = {Hydrogen {{Peroxide}} in {{Plants}}: A {{Versatile Molecule}} of the {{Reactive Oxygen Species Network}}},
  shorttitle = {Hydrogen {{Peroxide}} in {{Plants}}},
  author = {Quan, Li-Juan and Zhang, Bo and Shi, Wei-Wei and Li, Hong-Yu},
