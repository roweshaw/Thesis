  langid = {english},
  pmcid = {PMC94731},
  pmid = {11029417},
  keywords = {Aspartic Acid,Bacterial Proteins,Culture Media,Decanoates,Disaccharides,DNA-Binding Proteins,Fimbriae Proteins,Fimbriae; Bacterial,Flagella,Flagellin,Glutamic Acid,Histidine,main,Microscopy; Electron,Movement,Mutation,Nitrogen,Proline,Pseudomonas aeruginosa,Rhamnose,Signal Transduction,Surface-Active Agents,Trans-Activators,Transcription Factors},
